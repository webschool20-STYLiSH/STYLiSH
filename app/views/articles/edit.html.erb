<h1>投稿を編集</h1>
<%= form_for (@article) do |f| %>
	<%= f.hidden_field :user_id, :value => current_user.id %>
	タイトル
	<%= f.text_field :title, class:"..." %>
	コメント
	<%= f.text_field :body, class:"..." %>
	<%= f.submit "保存", class:"..." %>
<% end %>

<%= link_to "タイムラインへ戻る", articles_path, class: "btn" %>